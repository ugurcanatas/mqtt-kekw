{
  "name": "mqtt-kekw",
  "version": "1.0.0",
  "description": "A Node.js client that uses MQTT pub/sub model with a TCP Socket",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build-r": "rm -fr lib/* && rollup -c",
    "rollup-dev": "rm -fr lib/* && rollup -c rollup-test.config.js && node lib/index.js",
    "tsc": "rm -fr lib/* && tsc",
    "prepublishOnly": "npm run build-r"
  },
  "keywords": [
    "mqtt",
    "node.js",
    "client",
    "tcp",
    "ws"
  ],
  "author": "Uğurcan Emre Ataş",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/node": "^16.10.3",
    "rollup": "^2.58.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "typed-emitter": "^1.3.1",
    "typescript": "^4.4.3"
  }
}